Cell[TextData[{
 "The vector boson ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    SubscriptBox["A", "\[Mu]"], "a"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "e94fbca4-b2a0-49d3-bddc-8dab1fa08a99"],
 ", the auxiliary field ",
 Cell[BoxData[
